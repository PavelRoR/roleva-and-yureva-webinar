@media screen and (min-width 1900px)
    #main
        background-position: 73% 0
        background-size: 30%
    #know
        background-position: 100% bottom
@media screen and (min-width 2300px)
    .container
        width 1400px
    #main
    #post #main_screen
        background-position: 73% 32%
        padding 100px 0
    .pre
    .for_list li
    .know_list li
    #myPolitics .modal-title
    .link_for
        font-size: 24px
    .title_n_authors p
    #myPolitics .modal-body p
    .alert_message
    .mail
    .begin_date
        font-size: 22px
    .button
        font-size: 20px
        width: 425px;
        max-width 100%
    .for_after_title
        font-size: 40px
        max-width: 474px
    .checking .check_label
        font-size: 16px
    .button_mail
        width: 365px
    .post_title
        font-size: 43px
    #know
        background-position: 83% bottom;
    .know_list li
        height 72px
    #collage
        margin-top: 80px
@media screen and (max-width 1199px)
    .title_n_authors
        padding-left: 0
        background-position: 410px 148px
        max-width: 680px;
    #know
        background-position: 130px bottom
    #collage
        width: 400px
        margin-top: 100px
